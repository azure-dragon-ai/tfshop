{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "ext-bcmath": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-redis": "*",
        "ext-zip": "*",
        "alibabacloud/client": "^1.5",
        "doctrine/dbal": "^4.2.3",
        "iidestiny/laravel-filesystem-oss": "^3.6",
        "intervention/image": "^3.11",
        "johnlui/aliyun-oss": "~2.0",
        "laravel/ui": "^4.6",
        "maatwebsite/excel": "^3.1",
        "nyholm/psr7": "^1.3",
        "overtrue/laravel-wechat": "^7.4.0",
        "spatie/db-dumper": "^3.8",
        "spatie/laravel-backup": "^8.8.2",
        "webpatser/laravel-uuid": "^4.0",

        "php": "^8.1",
        "aliyuncs/oss-sdk-php": "^2.6",
        "aws/aws-sdk-php-laravel": "^3.9",
        "catfan/medoo": "^2.1",
        "darkaonline/l5-swagger": "^8.5",
        "dcat/easy-excel": "^1.1",
        "doctrine/annotations": "^2.0",
        "doctrine/cache": "^2.2",
        "gregwar/captcha-bundle": "^2.2",
        "guzzlehttp/guzzle": "^7.2",
        "hg/apidoc": "^5.2",
        "jenssegers/imagehash": "^0.8.0",
        "jianyan74/php-excel": "^1.0",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.21",
        "laravel/passport": "^12.0",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.21",
        "luomor/dcat-admin": "2.*",
        "luomor/openapidoc": "dev-main",
        "overtrue/laravel-filesystem-cos": "^3.4",
        "phpoffice/phpspreadsheet": "^1.29",
        "qcloud/cos-sdk-v5": "^2.6",
        "qiniu/php-sdk": "^7.11",
        "rtheunissen/guzzle-log-middleware": "^2.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "symfony/cache": "^6.4",
        "tencentcloud/tencentcloud-sdk-php": "^3.0",
        "tymon/jwt-auth": "2.0.0",
        "w7corp/easywechat": "^6.12.11"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.15",
        "beyondcode/laravel-dump-server": "^2.0.0",
        "filp/whoops": "^2.16",
        "knuckleswtf/scribe": "^5.1",
        "slevomat/coding-standard": "^8.18.0",
        "squizlabs/php_codesniffer": "^3.12",

        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/lib/alipay-sdk"
        ],
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {}
}
